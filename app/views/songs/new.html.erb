<h1>New Song:</h1>

  <%= form_for(@song) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :song[artist_id] %>
    <%= f.text_field :song[artist_id] %>
    <%= f.label :genre_id %>
    <%= f.text_field :genre_id %>
    <%= f.submit %>
  <% end %>

  <%= params.inspect %>
